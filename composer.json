{
  "name": "wy/google-font-cache",
  "version": "1.0.0",
  "description": "A WordPress Plugin to cache Google Fonts to your server to avoid GDPR conflicts.",
  "license": "MIT",
  "authors": [
    {
      "name": "Cem Derin",
      "email": "cem@wysiwyg.de"
    }
  ],
  "repositories":[
    {
      "type":"composer",
      "url":"https://wpackagist.org"
    }
  ],
  "require": {
    "johnpbloch/wordpress": "5.2.4",
    "composer/installers": "^1.7"
  },
  "extra": {
    "wordpress-install-dir": "./.hypress/htdocs/wp/",
    "installer-paths": {
      "./.hypress/htdocs/wp/wp-content/mu-plugins/{$name}/": [
        "type:wordpress-muplugin"
      ],
      "./.hypress/htdocs/wp/wp-content/plugins/{$name}/": [
        "type:wordpress-plugin"
      ],
      "./.hypress/htdocs/wp/wp-content/themes/{$name}/": [
        "type:wordpress-theme"
      ]
    }
  },
  "config": {
    "vendor-dir": "src/vendor"
  },
  "type": "wordpress-plugin"
}
